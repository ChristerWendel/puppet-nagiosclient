command[check_apt]     = /usr/lib/nagios/plugins/check_apt
command[check_disk]    = /usr/lib/nagios/plugins/check_disk -w 20% -c 10%
command[check_load]    = /usr/lib/nagios/plugins/check_load -w 5.0,4.0,3.0 -c 10.0,6.0,4.0
<% if @virtual == "physical" -%>
command[check_sensors] = /usr/lib/nagios/plugins/check_sensors
<% end -%>
